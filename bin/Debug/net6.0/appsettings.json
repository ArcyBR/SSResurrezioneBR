{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": { "Default": "Data Source=Data/SSResurrezione.db" },
  "SSResurrezione": {
    "PerPage": 3,
    "Order": {
      "Ascending": false,
      "Allow": [ "AlmanaccoDataEvento", "AlmanaccoTitolo","CoroPolifonicoMaterMisericordieDataEvento", "CoroPolifonicoMaterMisericordieTitolo" ]
    }
  },
  "CachedExpiredSeconds": 60,
  "MemoryCache": {
  },
  "DistributedCache:Redis": {
    "Configuration": "localhost",
    "InstanceName": "Almanaccos"
  },
  "UrlSitoSantoDelGiorno": "https://www.santodelgiorno.it/santi.json",
  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 5242880
    }
  },
  "ImgForCarouselImage": {
    "width": 546,
    "height": 410
  },
  "ImgAppuntamentiDellaSettimanaImage": {
    "width": 546,
    "height": 707
  }
}